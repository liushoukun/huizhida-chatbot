{
    "name": "huizhida/engine",
    "description": "HuiZhiDa Engine Package - conversation processing (core, channel, agent)",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "HuiZhiDa Team",
            "email": "team@huizhida.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "illuminate/support": "^12.0",
        "illuminate/queue": "^12.0",
        "illuminate/redis": "^12.0",
        "guzzlehttp/guzzle": "^7.0",
        "huizhida/core": "*"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "HuiZhiDa\\Engine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HuiZhiDa\\Engine\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HuiZhiDa\\Engine\\EngineServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0.x",
            "dev-main": "1.0.x",
            "dev-develop": "1.0.x"
        }
    }
}
