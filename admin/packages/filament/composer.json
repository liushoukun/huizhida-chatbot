{
    "name": "huizhida/filament",
    "description": "HuiZhiDa Filament Resources Package",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "HuiZhiDa Team",
            "email": "team@huizhida.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "huizhida/core":  "*",
        "red-jasmine/filament-project": "^1.0",
        "red-jasmine/filament-admin": "^1.0",
        "red-jasmine/filament-support": "^1.0",
        "spatie/laravel-package-tools": "^1.9"
    },
    "require-dev": {

    },
    "autoload": {
        "psr-4": {
            "HuiZhiDa\\Filament\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HuiZhiDa\\Filament\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HuiZhiDa\\Filament\\FilamentServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0.x",
            "dev-main": "1.0.x",
            "dev-develop": "1.0.x"
        }
    }
}
