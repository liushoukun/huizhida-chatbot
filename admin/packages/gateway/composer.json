{
    "name": "huizhida/gateway",
    "description": "HuiZhiDa Gateway Package",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "HuiZhiDa Team",
            "email": "team@huizhida.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "illuminate/support": "^12.0",
        "illuminate/queue": "^12.0",
        "illuminate/redis": "^12.0"
    },
    "require-dev": {

    },
    "autoload": {
        "psr-4": {
            "HuiZhiDa\\Gateway\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HuiZhiDa\\Gateway\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HuiZhiDa\\Gateway\\GatewayServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-main": "1.0.x-dev",
            "dev-develop": "1.0.x-dev"
        }
    }
}
